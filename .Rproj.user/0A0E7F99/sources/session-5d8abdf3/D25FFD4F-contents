setMethod("is_same_basis", "predictor_functional",
###########################################

function(input, other) {
if (
  !(
    prod(
      (input@J == other@J) * (input@J_dotdot == other@J_dotdot)
    )
  )
){
  return(FALSE)
  warning("Different bases")
}else{
  return(TRUE)
  }
}
###########################################
)
