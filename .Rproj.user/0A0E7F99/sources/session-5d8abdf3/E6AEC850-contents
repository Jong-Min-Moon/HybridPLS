setMethod("is_same", "basis_hybridPLS",
###########################################

function(input, other) {
            # basis check
if (
  prod( (input@J != other@J)) * prod(input@J_dotdot != other@J_dotdot) > 0
){
  return(FALSE)
  warning("Different bases")
}else{
  return(TRUE)
  }
}
###########################################
)
